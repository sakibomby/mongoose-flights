<%- include('../partials/header') %>

<form id="add-flight-to-ticket" action="/flights/<%= flightId %>/tickets" method="POST">
  <label>Seat:</label>
  <input name="seat" required pattern="[A-F][1-9]\d?">
  <label>Price:</label>
  <input type="number" name="price">
  <input type="submit" value="Add Ticket">
</form>

